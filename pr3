import array as arr
a=arr.array('d',[5.6,6.7,7.8]) #for float value
print(a)
#for add value in last
a.append(9.0)
print(a)
# for add value in given location
a.insert(2,7.8)
print(a)
#for pop value at given location
print("the popped value is:",end=" ")
print(a.pop(2))
print("array affter popping:",end=" ")
print(a)
#for remove vlue
print("the removed value is:",end=" ")
print(a.remove(9.0))
print(a)
